[{"_path":"/recipes/2024/2024-03-28","_dir":"2024","_draft":false,"_partial":false,"_locale":"","title":"Roasted chicken breast with lemon, thyme and garlic","description":"","image":"/gallery/2024/03/wnews20240328-069.jpg","excerpt":{"type":"root","children":[{"type":"element","tag":"h1","props":{"id":"roasted-chicken-breast-with-lemon-thyme-and-garlic"},"children":[{"type":"text","value":"Roasted chicken breast with lemon, thyme and garlic"}]}]},"body":{"type":"root","children":[{"type":"element","tag":"h1","props":{"id":"roasted-chicken-breast-with-lemon-thyme-and-garlic"},"children":[{"type":"text","value":"Roasted chicken breast with lemon, thyme and garlic"}]},{"type":"element","tag":"h3","props":{"id":"ingredients"},"children":[{"type":"text","value":"Ingredients"}]},{"type":"element","tag":"ul","props":{},"children":[{"type":"element","tag":"li","props":{},"children":[{"type":"text","value":"4 chicken breasts skin on"}]},{"type":"element","tag":"li","props":{},"children":[{"type":"text","value":"Salt and pepper"}]},{"type":"element","tag":"li","props":{},"children":[{"type":"text","value":"2 tablespoons olive oil"}]},{"type":"element","tag":"li","props":{},"children":[{"type":"text","value":"4 garlic cloves, finely chopped"}]},{"type":"element","tag":"li","props":{},"children":[{"type":"text","value":"2 teaspn finely chopped thyme leaves"}]},{"type":"element","tag":"li","props":{},"children":[{"type":"text","value":"2 teaspn dried oregano"}]},{"type":"element","tag":"li","props":{},"children":[{"type":"text","value":"1 lemon zest and juice"}]},{"type":"element","tag":"li","props":{},"children":[{"type":"text","value":"2 tablespoons olive oil, plus extra to serve"}]},{"type":"element","tag":"li","props":{},"children":[{"type":"text","value":"11/2 baby cos lettuce, leaves separated"}]},{"type":"element","tag":"li","props":{},"children":[{"type":"text","value":"Lemon wedges to serve"}]}]},{"type":"element","tag":"h3","props":{"id":"method"},"children":[{"type":